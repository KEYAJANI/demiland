{
  "name": "beauty-brand-wireframes",
  "version": "1.0.0",
  "description": "React wireframes for a modern beauty brand web store with showroom functionality",
  "private": true,
  "dependencies": {
    "@supabase/supabase-js": "^2.50.3",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "form-data": "^4.0.3",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "vite": "^5.0.8"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "NODE_ENV=production vite build",
    "build:staging": "NODE_ENV=staging vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test:build": "npm run build && npm run preview",
    "clean": "rm -rf dist node_modules/.vite",
    "deploy:setup": "cp .env.example .env.local && echo 'Please configure your .env.local file'",
    "health-check": "curl -f http://localhost:4173 || exit 1",
    "precommit": "npm run lint && npm run type-check",
    "postinstall": "echo 'Demiland project installed successfully. Run npm run deploy:setup to configure environment variables.'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "beauty",
    "cosmetics",
    "wireframes",
    "tailwindcss",
    "framer-motion",
    "showroom",
    "ecommerce",
    "modern",
    "responsive"
  ],
  "author": "Beauty Brand Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
